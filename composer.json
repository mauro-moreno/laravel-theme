{
  "name": "mauro-moreno/laravel-theme",
  "description": "Laravel 5 Themes: Asset & Views folder per theme. Theme inheritance. Blade integration and more... Fork of igaster/laravel-theme",
  "keywords": ["themes", "views", "assets", "blade", "laravel-5", "package"],
  "license": "MIT",
  "homepage": "https://github.com/mauro-moreno/laravel-theme",
  "authors": [
    {
      "name": "Giannis Gasteratos",
      "email": "igasteratos@gmail.com"
    },
    {
      "name": "Mauro Moreno",
      "email": "moreno.mauro.emanuel@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": ">=5.2.0"
  },
  "require-dev": {
    "illuminate/database": "~5.2",
    "orchestra/testbench": "~3.0",
    "vlucas/phpdotenv": "~2.0",
    "phpunit/phpunit": "~4.0"
  },
  "suggest": {
    "orchestra/asset" : "Use '@css' and '@js' in Blade files"
  },
  "autoload": {
    "psr-4" : {
      "MauroMoreno\\LaravelTheme\\" : "src/",
      "MauroMoreno\\LaravelTheme\\Tests\\" : "tests/"
    }
  },
  "minimum-stability": "stable"
}
